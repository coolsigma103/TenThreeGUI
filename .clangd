CompileFlags:
  Add:
    - -std=gnu++23

    - -Wno-unused-variable
    - -Wno-unused-function
    - -Wno-unused-parameter
    - -Wno-unused-local-typedef
    - -Wno-unused-const-variable
    - -Wno-unused-but-set-variable
    - -Wno-unused-macros

    - -Wno-missing-field-initializers
    - -Wno-sign-compare
    - -Wno-unknown-pragmas

Diagnostics:
  Suppress:
    - unused-includes
    - unused-variable
    - unused-function
    - unused-parameter
    - unused-namespace
